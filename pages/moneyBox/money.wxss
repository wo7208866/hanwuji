/* pages/moneyBox/money.wxss */
.rest-money{
  padding: 35px 0;
  position: relative;
  border-top: solid 1px #d6d6d6;
  background: #fff;
}

.rest-money image{
  width: 40px;
  height: 40px;
  margin-left: 30px;
  vertical-align: middle;
  background: #fff;
}
.rest-money text{
  margin-left: 10px;
  font-size: 16px;
  color: #bb9a6b;

}
.strong{
  font-weight: bold;
}
.go-for{
   position: absolute;
   right: 30px;
   width: 20px!important;
   height: 30px!important;
   background: #fff;
   margin-top: 4px;
}
.add-money{
  padding: 15px 0;
  background: #fff;
  border-top: solid 1px #d6d6d6;
  border-bottom: solid 5px #ebebeb;
}
.add-money view{
  display: inline-block;
  width: 50%;
}
.add-money image{
  vertical-align: middle;
}
.add-money text{
  color: #bb9a6b;
  vertical-align: middle;
}
.add-money-text1{
  text-align: center;
  font-size: 16px;
  display: inline-block;
  vertical-align: middle;
}
.add-money-text1 image{
  width: 30px;
  height: 30px;
  background: #fff;
  margin-right: 10px;
}
.bar-line{
    border: solid 1pt #bb9a6b;
    font-weight: bold;
    width: 20px!important;
    height: 20px!important;  
    border-radius: 50% 50%;
    text-align: center;
    line-height: 17px;
    font-size: 16px;
}

.border-line{
 border-left: solid 1px #dbd5d1;
 box-sizing: border-box;
 text-align: center;
}
.add-money-text3{
  text-align: center;
 vertical-align: middle;
}
.add-money-text3 image{
  width: 30px;
  height: 30px;
  background: #fff;
}
.add-money-text3 text{
  margin-left: 10px;
}
.money-list{
  padding:10px 30px;
  position: relative;
  background: #fff;
   border-top: solid 1px #d6d6d6;
}
.money-list image{
  width: 40px;
  height: 40px;
  vertical-align: middle;
  background: #fff;
}
.money-text1{
  margin-left: 30px;
}
.money-text2{
  position: absolute;
  right: 30px;
  top: 15px;
  width: 20px!important;
  height: 30px!important;
  background: #fff;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 10rpx;
}

.modal-content {
  padding: 50rpx 32rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  background: #222222;
  color: white;
  font-size:11pt;
  text-align: center;
}

.btn-confirm {
  width: 50%;
  background:linear-gradient(to right,#DDCAA0,#BB9A6B);
  color: white;
  font-size:11pt;
  text-align: center;
}
.navigator-hover{
  background: none!important;
}
